func fib(x Int) Int {
    var y Int;

    if x <= 2 {
        return 1;
    } 
    else{
        y=1;
    }

    return fib(x-1) + fib(x-2);
}


// ok main
func main() Void{
    var i Int;
    var j Int;
    for i = 1;i < 15; i = i + 1 {
        printf("fib(%lld) = %lld \n", i, fib(i));
    }

    return;
}

